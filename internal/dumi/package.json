{
  "name": "dumi",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@akrc/seaweed": "workspace:*",
    "dumi": "^2.0.13",
    "lit": "2.5.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "scripts": {
    "prebuild": "pnpm run -w build:package",
    "watch": "pnpm run prebuild --watch",
    "dev": "cross-env NODE_ENV=development npm-run-all -p watch dev:dumi",
    "dev:dumi": "dumi dev",
    "prepare": "dumi setup",
    "build": "lerna run prebuild && dumi build",
    "build:ci": "pnpm run -w build:package && dumi build"
  }
}
