{
    "compilerOptions": {
        "experimentalDecorators": true,
        "moduleResolution": "node",
        "target": "ESNext",
        "useDefineForClassFields": false,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "module": "ESNext",
        "paths": {
            "@/*": [
                "./src/*"
            ],
            "#/*": [
                "./style/*"
            ]
        }
    },
}