{
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "prebuild",
          "build"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "{projectRoot}/dist",
        "{projectRoot}/internal/storybook/storybook-static",
        "{projectRoot}/internal/dumi/dist"
      ]
    },
    "prebuild": {
      "outputs": [
        "{projectRoot}/dist"
      ]
    }
  }
}
